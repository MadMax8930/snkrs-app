{
   "version": 2,
   "builds": [
     {
       "src": "next.config.js",
       "use": "@vercel/next"
     },
     {
       "src": "src/api/index.js",
       "use": "@vercel/node"
     }
   ],
   "routes": [
     {
       "src": "/api/(.*)",
       "dest": "src/api/index.js"
     },
     {
       "src": "/(.*)",
       "dest": "next.config.js"
     }
   ]
 }
 